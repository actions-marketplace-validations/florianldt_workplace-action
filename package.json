{
    "name": "workplace-action",
    "version": "0.0.1",
    "description": "GitHub Action for sending a Workplace message.",
    "main": "lib/index.js",
    "scripts": {
        "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
        "build": "tsc",
        "format": "prettier --write '**/*.ts'",
        "lint": "eslint .",
        "package": "ncc build --license licenses.txt",
        "prepare": "husky install",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/florianldt/workplace-action.git"
    },
    "keywords": ["workplace", "action", "message"],
    "author": "Florian Ludot",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/florianldt/workplace-action/issues"
    },
    "homepage": "https://github.com/florianldt/workplace-action#readme",
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@types/jest": "^28.1.8",
        "@types/node": "^18.7.13",
        "@typescript-eslint/eslint-plugin": "^5.35.1",
        "@typescript-eslint/parser": "^5.35.1",
        "@vercel/ncc": "^0.34.0",
        "dotenv": "^16.0.1",
        "eslint": "^8.23.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.8.7",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.0",
        "jest": "^28.1.3",
        "nodemon": "^2.0.19",
        "ts-jest": "^28.0.8",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@actions/core": "^1.9.1",
        "axios": "^0.27.2"
    }
}
